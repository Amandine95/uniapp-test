<view><view><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" bindsubmit="__e"><view class="uni-flex uni-row"><view class="title">城市:</view><picker mode="region" range="{{city}}" name="city" data-event-opts="{{[['change',[['cityChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{city}}</view></picker></view><view class="uni-flex uni-row"><view class="title">用途:</view><picker value="{{usage[index]}}" range="{{usage}}" name="use" data-event-opts="{{[['change',[['usageChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{usage[index]}}</view></picker></view><view class="uni-flex uni-row"><view class="title">基准日:</view><picker mode="date" range="{{date}}" start="1900-12-31" end="2099-12-31" name="date" data-event-opts="{{[['change',[['dateChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{date}}</view></picker></view><view class="uni-flex uni-row"><view class="title">房产位置:</view><input type="text" name="addr" placeholder="请输入地址" value="{{addr}}"/></view><view class="uni-row"><map style="width:750rpx;height:300rpx;" latitude="{{lat}}" longitude="{{lon}}" markers="covers" data-event-opts="{{[['tap',[['openMap',['$event']]]]]}}" bindtap="__e"></map></view><view class="uni-btn-v uni-common-mt"><button class="btn-submit" form-type="submit" type="primary">下一步</button></view></form></view></view>